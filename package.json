{
  "name": "eslint-config-epsvue",
  "version": "1.0.4",
  "description": "A reusable and shareable `eslint, prettier and stylelint` configuration for Vue project",
  "main": "lib/index.js",
  "scripts": {
    "get-version": "node -p \"require('./package').version\"",
    "publish:beta": "npm publish --tag beta",
    "format": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stylelint": "stylelint --aei '**/*.{css,vue,scss}'",
    "test": "node --test",
    "watch": "npm test --ignore-scripts -- --watch"
  },
  "files": [
    "lib",
    "stylelint"
  ],
  "keywords": [
    "eslint",
    "prettier",
    "vue",
    "stylelint",
    "configuration",
    "eslint-config-epsvue",
    "stylelint-config"
  ],
  "author": {
    "name": "it-wibrc",
    "email": "wibrc.se.jc@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IT-WIBRC/eslint-config-epsvue.git"
  },
  "peerDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^2.8.3",
    "stylelint": "^14.2.0"
  },
  "dependencies": {
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-storybook": "^0.5.12",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-standard-vue": "^1.0.0"
  },
  "stylelint": {
    "extends": "./stylelint/index.js"
  },
  "eslintConfig": {
    "extends": [
      "./lib/index.js"
    ]
  },
  "engineStrict": true,
  "devDependencies": {
    "mocha": "^10.8.2"
  }
}
