{
  "name": "eslint-config-epsvue",
  "version": "1.2.1",
  "description": "A reusable and shareable `eslint` configuration for Vue project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "get-version": "node -p \"require('./package').version\"",
    "publish:beta": "npm publish --tag beta",
    "format": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stylelint": "stylelint '**/*.{css,vue,scss}'"
  },
  "keywords": [
    "eslint",
    "prettier",
    "vue",
    "stylelint",
    "configuration",
    "eslint-config-epsvue",
    "stylelint-config",
    "create-eslint-config"
  ],
  "author": {
    "name": "it-wibrc",
    "email": "wibrc.se.jc@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IT-WIBRC/eslint-config-epsvue.git"
  },
  "peerDependencies": {
    "eslint": ">=9",
    "prettier": "^3.3.0",
    "stylelint": "^15.0.0 || ^16.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.14.0",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.11.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^6.5.0",
    "typescript-eslint": "^8.14.0"
  },
  "eslintConfig": {
    "extends": [
      "./eslint.config.mjs"
    ]
  },
  "stylelint": {
    "extends": "./.stylelintrc.json"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=18.18.0"
  }
}
