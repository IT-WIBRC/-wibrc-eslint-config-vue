{
  "name": "@gge/eslint-config-epsvue",
  "version": "1.0.0",
  "description": "A reusable and shareable `eslint` configuration for Vue project",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint --fix",
    "stylelint": "stylelint '**/*.css'",
    "get-version": "node -p \"require('./package').version\"",
    "publish:beta": "npm publish --tag beta"
  },
  "keywords": [
    "eslint",
    "prettier",
    "vue",
    "stylelint",
    "configuration"
  ],
  "author": {
    "name": "it-wibrc",
    "email": "wibrc.se.jc@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/IT-WIBRC/eslint-config-epsvue"
  },
  "peerDependencies": {
    "eslint": "^8.32.0",
    "prettier": "^2.8.3",
    "stylelint": "^14.2.0"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-vue": "^8.2.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-scss": "^4.1.0"
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.js"
    ]
  },
  "engineStrict": true
}
