{
  "name": "eslint-config-epsvue",
  "version": "1.0.0",
  "description": "A reusable and shareable `eslint, prettier and stylelint` configuration for Vue project",
  "main": "index.js",
  "scripts": {
    "get-version": "node -p \"require('./package').version\"",
    "publish:beta": "npm publish --tag beta"
  },
  "keywords": [
    "eslint",
    "prettier",
    "vue",
    "stylelint",
    "configuration",
    "eslint-config-epsvue",
    "stylelint-config"
  ],
  "author": {
    "name": "it-wibrc",
    "email": "wibrc.se.jc@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IT-WIBRC/eslint-config-epsvue.git"
  },
  "peerDependencies": {
    "eslint": "^8.32.0",
    "prettier": "^2.8.3",
    "stylelint": "^14.2.0"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.30.0",
    "eslint-plugin-storybook": "^0.11.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.4.0",
    "stylelint-scss": "^6.5.0"
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.js"
    ]
  },
  "engineStrict": true
}
